
begin
  class ordsimple
    int x
    int u 
    int t
    u=x
    for 0...x do
     u=t
     int f(u,t)
     
  end
 class Picard
  def phi
      int x
      int v
      int i
      for i in....x do
       int t
       v=t
       t
       t=v
      end
 end
 class Discretization
  int u
  int n
  int i
  int t
  int f
  def delta
   t 
   int t2
   t2=t
   t2-t
  end
   for i in n....1 do
    u + delta*f(u,t)
 end
end
